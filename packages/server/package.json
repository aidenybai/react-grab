{
  "name": "@react-grab/server",
  "version": "0.0.49",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.js"
      },
      "require": {
        "types": "./dist/types.d.cts",
        "default": "./dist/types.cjs"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/client.d.ts",
        "default": "./dist/client.js"
      },
      "require": {
        "types": "./dist/client.d.cts",
        "default": "./dist/client.cjs"
      }
    },
    "./dist/*": "./dist/*.js",
    "./dist/*.js": "./dist/*.js",
    "./dist/*.cjs": "./dist/*.cjs",
    "./dist/*.mjs": "./dist/*.mjs"
  },
  "bin": {
    "react-grab-server": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.45",
    "@hono/node-server": "^1.14.4",
    "ai": "^5.0.95",
    "ai-sdk-provider-claude-code": "^2.1.0",
    "hono": "^4.7.7",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^20.19.23",
    "tsup": "^8.2.4"
  }
}
