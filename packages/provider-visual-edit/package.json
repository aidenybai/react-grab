{
  "name": "@react-grab/visual-edit",
  "version": "0.0.87",
  "type": "module",
  "bin": {
    "react-grab-visual-edit": "./dist/cli.cjs"
  },
  "exports": {
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "rm -rf dist && NODE_ENV=production tsup"
  },
  "devDependencies": {
    "tsup": "^8.4.0"
  },
  "dependencies": {
    "@types/node": "^24.10.2",
    "picocolors": "^1.1.1",
    "react-grab": "workspace:*"
  }
}
