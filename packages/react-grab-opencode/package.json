{
  "name": "@react-grab/opencode",
  "version": "0.0.67",
  "description": "Opencode integration for React Grab - Send UI element context directly to Opencode terminal AI agent",
  "keywords": [
    "react",
    "grab",
    "opencode",
    "agent",
    "ai",
    "terminal"
  ],
  "homepage": "https://react-grab.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aidenybai/react-grab.git",
    "directory": "packages/react-grab-opencode"
  },
  "license": "MIT",
  "author": {
    "name": "Aiden Bai",
    "email": "aiden@million.dev"
  },
  "type": "module",
  "exports": {
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./dist/*": "./dist/*.js",
    "./dist/*.js": "./dist/*.js"
  },
  "browser": "dist/client.global.js",
  "bin": {
    "react-grab-opencode": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@opencode-ai/sdk": "^1.0.0",
    "hono": "^4.0.0",
    "picocolors": "^1.1.1",
    "react-grab": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.4.0"
  }
}
